function VowelSquare(strArr) { 

//Input check
if(strArr.length > 1) {
    
  //Check for the non letters symbols    
  if(irritation2DArr(strArr)) {
      //All letters
      
  } else {
      
      
  }
}

  // code goes here  
  return strArr; 
        
}

//2D array irritation
const irritation2DArr = array => {
    let nonLettersIndex = [];
    let allVowelIndex =[];
    for(let i = 0; i < array.length; i++) {
        for (let j = 0; j < array[i].length; j++) {
            
            
            //Save the position of non-letter character
            if(!testFunc(array[i][j])) {
                symbol.push([i,j]);
            }
            
            //Save the position of all vowels characters
            if(vowelsPossition(array[i][j])) {
                allVowelIndex.push([i,j]);
            }
        }
    
    }
    console.log(allVowelIndex); 
    
    return nonLettersIndex.length > 0 ?  nonLettersIndex : true;
}

//Test function for all arrays
const testFunc = char => char.match(/[a-zA-Z]/);


//Creating 2D array with positions of all vowels in array
const vowelsPossition = char => 
char.toLowerCase() === 'a' || char.toLowerCase() === 'e' || char.toLowerCase() === 'i' || char.toLowerCase() === 'o' || char.toLowerCase() === 'u'
   
// keep this function call here 
VowelSquare(readline());
